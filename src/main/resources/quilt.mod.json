{
	"schema_version": 1,
	"quilt_loader": {
		"group": "com.halotroop",
		"id": "omnimod",
		"version": "${version}",
		"metadata": {
			"name": "Omnimod",
			"description": "Every mod in one.",
			"contributors": {
				"halotroop2288": "Owner"
			},
			"contact": {
				"homepage": "https://halotroop.com/caroline/projects/omnimod",
				"sources": "https://github.com/halotroop2288/Quilt-Omnimod",
				"issues": "https://github.com/halotroop2288/Quilt-Omnimod/issues"
			},
			"icon": "assets/omnimod/icon.png"
		},
		"intermediate_mappings": "net.fabricmc:intermediary",
		"entrypoints": {
			"init": [
				"com.halotroop.omnimod.Omnimod",
				"com.halotroop.omnimod.commands.OmnimodCommands",
				"com.halotroop.omnimod.world.level.block.OmnimodBlocks",
				"com.halotroop.omnimod.world.level.block.entity.OmnimodBlockEntityType",
				"com.halotroop.omnimod.world.entity.OmnimodEntityType",
				"com.halotroop.omnimod.world.item.OmnimodItems",
				"com.halotroop.omnimod.world.level.levelgen.feature.OmnimodOreFeatures"
			],
			"client_init": [
				"com.halotroop.omnimod.client.OmnimodClient"
			],
			"server_init": [
				"com.halotroop.omnimod.server.OmnimodServer"
			]
		},
		"language_adapters": {
			"groovy": "one.codehz.language.groovy.GroovyLanguageAdapter"
		},
		"depends": [
			{
				"id": "java",
				"versions": "17"
			},
			{
				"id": "quilt_loader",
				"versions": ">=0.17.0"
			},
			{
				"id": "minecraft",
				"versions": "1.18.2"
			},
			{
				"id": "quilted_fabric_api",
				"versions": ">=1.0.0-"
			}
		],
		"breaks": [
			{
				"id": "sodium",
				"reason": "Sodium does not implement the Quilt Rendering API.",
				"unless": "indium"
			},
			{
				"id": "optifabric",
				"reason": "Optifine patches everything, which breaks everything."
			}
		]
	},
	"mixin": "omnimod.mixins.json"
}
